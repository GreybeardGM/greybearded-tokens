<div class="gbt-frames__sync-dialog">
  <p class="gbt-frames__sync-dialog-text">{{text}}</p>

  <div class="gbt-frames__sync-grid">
    <section class="gbt-frames__sync-card">
      <h3 class="gbt-frames__sync-label">{{actorPortraitLabel}}</h3>
      <div class="gbt-frames__sync-image-wrap">
        <img src="{{actorPortraitSrc}}" alt="{{actorPortraitLabel}}" class="gbt-frames__sync-image" />
      </div>
      <code class="gbt-frames__sync-path">{{actorPortraitPath}}</code>
    </section>

    <section class="gbt-frames__sync-card">
      <h3 class="gbt-frames__sync-label">{{protoTokenLabel}}</h3>
      <div class="gbt-frames__sync-image-wrap">
        <img src="{{protoTokenSrc}}" alt="{{protoTokenLabel}}" class="gbt-frames__sync-image" />
      </div>
      <code class="gbt-frames__sync-path">{{protoTokenPath}}</code>
    </section>
  </div>
</div>
